{
    "name": "express-ts-crud",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "dev": "NODE_ENV=dev ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "NODE_ENV=prod node dist/index.js",
        "migration:run": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js migration:run -d src/configs/data-source.ts",
        "test": "NODE_ENV=test jest"
    },
    "dependencies": {
        "express": "^4.18.2",
        "mysql2": "^3.9.1",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.19"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.4.2",
        "@types/supertest": "^2.0.16",
        "jest": "^29.7.0",
        "supertest": "^7.1.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.6"
    }
}
